{
	"name": "template-daisy-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.11.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.2",
		"@testing-library/svelte": "^5.3.1",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"@vitest/ui": "^4.0.18",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.22",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^17.2.0",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"lint-staged": "^16.2.7",
		"postcss": "^8.4.49",
		"postcss-nesting": "^14.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.12.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.16",
		"terser": "^5.46.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vitest": "^4.0.18"
	},
	"type": "module",
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,css,md}": [
			"prettier --write"
		]
	}
}
